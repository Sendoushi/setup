Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

URxvt.modifier: alt
URxvt.internalborder: 2
URxvt.depth: 32
URxvt.background: #000000                                                                   
URxvt.foreground: #E4E4E4                                                                   
URxvt.scrollBar: false      
URxvt.mouseWheelScrollPage: true
URxvt.saveLines: 32767
URxvt.urgentOnBell: true

URxvt.url-select.launcher: web_browser

!URxvt.font: xft:DejaVu Sans Mono:pixelsize=12:antialias=true
!URxvt.font: xft:Courier New:pixelsize=12:antialias=true
URxvt*font: xft:Noto Mono:pixelsize=14:antialias=true,xft:Monospace:pixelsize=12:antialias=true
URxvt.allow_bold: true
URxvt.letterSpace: -2
URxvt.lineSpace: 2
URxvt*fading: 50

# Plugins

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard
